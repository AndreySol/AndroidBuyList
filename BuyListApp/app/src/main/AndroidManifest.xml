<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.buylist.solomakha.buylistapp"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name="com.buylist.solomakha.buylistapp.MainApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.BasketListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.ProductListActivity">
        </activity>

        <service android:name=".ui.MyIntentService"/>
    </application>

</manifest>
